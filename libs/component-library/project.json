{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/component-library/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outPath}"],"defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "baseHref": "/",
        "main": "libs/component-library/src/index.ts",
        "outputPath": "dist/libs/component-library",
        "tsConfig": "libs/component-library/tsconfig.json",
        "index": "libs/component-library/src/index.ts",
        "styles": ["libs/style-guide/src/styles.scss"],
        "stylePreProcessorOptions": {
          "includePaths": ["libs/style-guide/src/styles.scss"]
        },
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "libs/component-library/src/environments/environment.ts",
              "with": "libs/component-library/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/component-library/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/component-library"],
      "options": {
        "jestConfig": "libs/component-library/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
